{% extends 'BlogBundle::layout.html.twig' %}
{% block title %}
   Accueil -{{ parent() }} 
{% endblock%}
{% block blog_body %}
     {% include "BlogBundle:Blog:article.html.twig" %} 
    
    <div class="pagination">
<ul>
    {% for p in range(1, nombrePage) %}
<li{% if p == page %} class="active"{% endif %}>
<a href="{{ path('blog_accueil', {'page': p}) }}">{{ p}}</a>

</li>
{% endfor %}
</ul>
</div>
    
{% endblock %}